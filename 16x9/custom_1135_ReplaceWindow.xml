<?xml version="1.0" encoding="UTF-8"?>
<window id="1135" type="window">
	<onload condition="String.IsEqual(Window(Home).Property(ReplaceWindow),Path)">ReplaceWindow(Videos,$ESCINFO[Window(Home).Property(ContainerPath)],return)</onload>
	<onunload>ClearProperty(ContainerPath,Home)</onunload>
	<onunload>ClearProperty(ReplaceWindow,Home)</onunload>
	<controls>
		<control type="image">
			<include>FullscreenDimensions</include>
			<animation effect="fade" time="300">WindowOpen</animation>
			<animation effect="fade" time="300">WindowClose</animation>
			<texture>black.png</texture>
		</control>
	</controls>
</window>